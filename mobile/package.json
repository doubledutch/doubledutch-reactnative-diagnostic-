{
  "name": "doubledutch-diagnostic",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/dd-rn-packager/react-native/local-cli/cli.js start --port 8085",
    "ios": "node node_modules/dd-rn-packager/react-native/local-cli/cli.js run-ios",
    "android": "node node_modules/dd-rn-packager/react-native/local-cli/cli.js run-android",
    "web": "webpack-dev-server --port 3000 --config web/webpack.config.dev.js -d --inline --hot --colors",
    "clean-web": "rm -Rf web/static/build/",
    "build-web": "npm run clean-web && webpack --colors --display-error-details --config web/webpack.config.prod.js",
    "build-web-old": "webpack --colors --display-error-details --config web/webpack.config.prod.js"
  },
  "dependencies": {
    "react-native": "0.46.4",
    "react-native-camera": "^0.9.4",
    "react-native-fetch-blob": "^0.10.5",
    "react-native-progress": "^3.2.1",
    "react-native-video": "^1.0.0",
    "react-native-youtube": "^1.0.0-beta.1",
    "@horizon/client": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "base-64": "^0.1.0",
    "bazaar-client": "^0.46.2",
    "json-loader": "^0.5.4",
    "react-native-cached-image": "^1.3.3",
    "react-navigation": "^1.0.0-beta.9",
    "soap": "^0.18.0",
    "ws": "^1.1.1",
    "react": "16.0.0-alpha.12",
    "react-addons-update": "15.6.0"
  },
