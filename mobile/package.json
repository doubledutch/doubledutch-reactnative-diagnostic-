    "babel-plugin-module-rewrite": "^0.1.2",
    "babel-plugin-replace-require": "0.0.4",
    "dd-rn-packager": "^0.46.8",
    "file-loader": "^0.10.0",
    "fs.realpath": "^1.0.0",
    "has-flag": "^2.0.0",
    "path-override-webpack-plugin": "^0.1.2",
    "prepack": "^0.1.1",
